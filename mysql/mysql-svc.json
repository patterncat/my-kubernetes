{
    "apiVersion": "v1",
    "kind": "Service",
    "metadata": {
        "name": "mysql-service",
        "namespace": "default",
        "labels": {
            "name": "mysql-service"
        }
    },
    "spec": {
        "selector": {
            "app": "mysql"
        },
        "ports": [
            {
                "protocol": "TCP",
                "port": 3306
            }
        ]
    }
}

